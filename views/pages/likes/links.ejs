  <% posts.forEach(function(post){ %>
    <ul>
         <li><%= post.blogName %></li>
         <% if (post.linkImage) { %>
            <a href="<%= post.url %>"><img src="<%= post.linkImage %>"></a>
         <% } %>
         <p><%= post.publisher %></p>
         <h4><a href="<%= post.url %>"><%= post.title %></a></h4>
         <p><%= post.excerpt %> </p>
         <p>By <%= post.linkAuthor %> </p>
    </ul>
  <% }); %>