
  <h2> Photos </h2>

     <% posts.forEach(function(post){ %>
        <div class="post-wrapper col-md-7">
            <div class="blogname-text">
              <p class="blogname"><%= post.blogName %></p>
            </div>
            <div class="photo">
              <img class="image" src="<%= post.photoUrl %>">
            </div>
        </div>
     <% }); %>
